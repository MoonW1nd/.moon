{
  "title": "rcmd to opt + shift",
  "rules": [
    {
      "manipulators": [
        {
          "description": "Change right command to option+shift.",
          "from": {
            "key_code": "right_option",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_option",
              "modifiers": ["left_shift"]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
